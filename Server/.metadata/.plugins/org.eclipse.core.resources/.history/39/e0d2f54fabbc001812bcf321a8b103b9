  <!-- 处理用户相关信息的userServlet -->
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<servlet-class>main.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <!-- 对时间分配表进行操作的userServlet -->
  <servlet>
  	<servlet-name>TimeSharingServlet</servlet-name>
  	<servlet-class>main.servlet.TimeSharingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TimeSharingServlet</servlet-name>
  	<url-pattern>/TimeSharingServlet</url-pattern>
  </servlet-mapping>
